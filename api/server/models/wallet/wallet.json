{
  "name": "wallet",
  "plural": "wallets",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "balance": {
      "type": "number",
      "required": true,
      "default": 0.0,
      "postgresql": {
        "dataType": "BIGINT"
      }
    },
    "address": {
      "type": "string",
      "default": "\"\""
    },
    "updatedAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "primaryKey": "id",
      "model": "user",
      "type": "belongsTo"
    },
    "asset": {
      "primaryKey": "ticker",
      "model": "asset",
      "type": "belongsTo"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "super_admin",
      "permission": "ALLOW"
    }],
  "methods": {}
}
