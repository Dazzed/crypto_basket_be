{
  "name": "transfer",
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "coin": {
      "required": true,
      "type": "string"
    },
    "txid": {
      "required": true,
      "type": "string"
    },
    "createdAt": {
      "defaultFn": "now",
      "type": "date"
    },
    "confirmedTime": {
      "type": "date"
    },
    "usdValue": {
      "required": true,
      "type": "number",
      "postgresql": {
        "dataType": "NUMERIC(10,2)"
      }
    },
    "sourceAddress": {
      "required": true,
      "type": "string"
    },
    "destAddress": {
      "required": true,
      "type": "string"
    },
    "value": {
      "required": true,
      "type": "number",
      "postgresql": {
        "dataType": "BIGINT"
      }
    },
    "txHash":{
      "required":false,
      "type": "string"
    },
    "confirmed":{
      "required": false,
      "type": "boolean",
      "default": false
    }
  },
  "hidden": [],
  "relations": {
    "wallet": {
      "primaryKey": "id",
      "model": "wallet",
      "type": "belongsTo"
    },
    "user":{
      "primaryKey": "id",
      "model": "user",
      "type": "belongsTo"
    }
  },
  "indexes": {},
  "methods": {},
  "acls": [
    {
      "property": "webhook",
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "super_admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "property": "find",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "mixins": {
    "SetupRemoteMethods": {}
  }
}
