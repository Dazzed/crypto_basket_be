{
  "name": "trade",
  "properties": {
    "quantity": {
      "required": true,
      "type": "number",
      "postgresql": {
        "dataType": "NUMERIC(10,2)"
      }
    },
    "exchangeRate": {
      "required": true,
      "type": "number",
      "postgresql": {
        "columnName": "exchangeRate",
        "dataType": "NUMERIC(10,2)"
      }
    },
    "fromAssetAmount": {
      "required": true,
      "type": "number",
      "postgresql": {
        "columnName": "fromAssetAmount",
        "dataType": "NUMERIC(10,2)"
      }
    },
    "toAssetAmount": {
      "required": true,
      "type": "number",
      "postgresql": {
        "columnName": "toAssetAmount",
        "dataType": "NUMERIC(10,2)"
      }
    },
    "createdAt": {
      "defaultFn": "now",
      "type": "date",
      "postgresql": {
        "columnName": "createdAt"
      }
    },
    "updatedAt": {
      "defaultFn": "now",
      "type": "date",
      "postgresql": {
        "columnName": "updatedAt"
      }
    },
    "completedAt": {
      "type": "date",
      "postgresql": {
        "columnName": "completedAt"
      }
    }
  },
  "hidden": [],
  "relations": {
    "user": {
      "primaryKey": "id",
      "model": "user",
      "type": "belongsTo"
    },
    "fromAsset": {
      "primaryKey": "id",
      "model": "asset",
      "type": "belongsTo"
    },
    "toAsset": {
      "primaryKey": "id",
      "model": "asset",
      "type": "belongsTo"
    }
  },
  "indexes": {},
  "methods": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "super_admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": "initiateTrade"
    }
  ],
  "mixins": {
    "SetupRemoteMethods": {
      "disableAllExcept": [
        "initiateTrade"
      ]
    }
  }
}